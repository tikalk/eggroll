{
  "name": "eggroll",
  "version": "0.0.1",
  "author": [
    "Oren Farhi",
    "Meital Federgreen",
    "Shimi Bar"
  ],
  "description": "a ui editor for writing cucumber specs and save it to github",
  "scripts": {
    "start": "gulp copy && gulp webserver",
    "test": "gulp test",
    "bundle": "browserify src/client/app/index.js -d -t babelify -p [minifyify --map bundle.map.json  --output .tmp/bundle.map.json] --outfile .tmp/bundle.js",
    "bundle-watch": "watchify -d -v -t babelify -e src/client/app/index.js -p [minifyify --map bundle.map.json  --output .tmp/bundle.map.json] -o .tmp/bundle.js",
    "release": "gulp copy && gulp browserify:dev && gulp dist && git checkout gh-pages && gulp copy:dist"
  },
  "keywords": [],
  "dependencies": {
    "material-design-lite": "^1.0.0-3",
    "octokat": "^0.4.10",
    "vue": "^0.12.7"
  },
  "devDependencies": {
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "css-loader": "^0.15.5",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-webserver": "^0.9.1",
    "material-design-lite": "^1.0.0-3",
    "minifyify": "^7.0.5",
    "node-libs-browser": "^0.5.2",
    "partialify": "^3.1.5",
    "style-loader": "^0.12.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1",
    "webpack": "^1.10.1"
  },
  "bundledDependencies": [],
  "engines": {
    "node": "0.12.x"
  }
}
